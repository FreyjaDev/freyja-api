{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@freyja-models/freyja-models/(.*)": "<rootDir>/../libs/freyja-models/src/$1",
    "@freyja-models/freyja-models": "<rootDir>/../libs/freyja-models/src"
  }
}